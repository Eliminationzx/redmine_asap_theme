<div class="ml-60">
  <div class="bg-white dark:bg-slate-700 shadow">
    <div data-controller="contextual" class="contextual px-4 py-2 grid grid-cols-3 justify-items-stretch gap-0 w-full">
      <div class="py-2">
        <%= custom_field_title @custom_field %>
      </div>
      <div></div>
      <div class="flex w-full">

      </div>
    </div>
  </div>

  <div class="mx-4 pb-16">


  <%= labelled_form_for :custom_field, @custom_field, :url => custom_field_path(@custom_field), :html => {:method => :put, :id => 'custom_field_form'} do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <% end %>
  </div>
</div>