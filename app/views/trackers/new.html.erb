<div class="ml-60">
  <div class="bg-white dark:bg-slate-700 shadow">
    <div data-controller="contextual" class="contextual px-4  py-2 grid grid-cols-3 justify-items-stretch gap-0 w-full">
      <div class="py-2">
        <%= title [l(:label_tracker_plural), trackers_path], l(:label_tracker_new) %>
      </div>
      <div></div>
      <div class="flex w-full justify-end">
      </div>
    </div>
  </div>

  <div class="mx-4">
    <%= labelled_form_for @tracker do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <% end %>
  </div>
</div>